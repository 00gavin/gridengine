#*******************************
proc output_monitor_result {start_size end_size} {
   global MONITOR_SIZE_ERROR
   global size_unit

   if { $start_size < $end_size } {
      set_error $MONITOR_SIZE_ERROR "size of qmaster increased from $start_size $size_unit to $end_size $size_unit" 
   } else {
      if { $start_size == $end_size } {
         set_error 0 "size of qmaster remained $end_size $size_unit"
      } else {
         set_error 0 "size of qmaster reduced from $start_size $size_unit to $end_size $size_unit"
      }
   }
}

