#*******************************
proc stabilize {text delay interval commands} {
   global CHECK_OUTPUT

   puts $CHECK_OUTPUT "waiting $delay s for system to stabilize $text"

   set end [expr [clock seconds] + $delay]
   set now 0
   while { $now < $end } {
      foreach i $commands {
         eval $i
      }
      monitor_size [get_job_count]
      sleep $interval
      set now [clock seconds]
   }   
}

