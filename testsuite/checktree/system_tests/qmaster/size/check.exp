proc get_last_jobid {} {
   global qmaster_spool_dir

   set seqfile $qmaster_spool_dir/jobseqnum

   if {![file exists $seqfile]} {
      set last_job 0
   } else {
      set f [open $seqfile r]
      if {[gets $f last_job] <= 0} {
         set last_job 0
      }
      close $f
   }

   return $last_job
}

