SGEOBJLIB_DIR = $(SDIR)/libs/sgeobj

SGEOBJLIB = libsgeobj$(QIDLEXT)$(LIBEXT)

ALLLISTSLIB = libsgeobjd$(QIDLEXT)$(LIBEXT)

SGEOBJLIB_SRC = \
         config.c \
         cull_parse_util.c \
         parse.c \
         sge_answer.c \
         sge_calendar.c \
         sge_complex.c \
         sge_conf.c \
         sge_ckpt.c \
         sge_event.c \
         sge_feature.c \
         sge_host.c \
         sge_hostgroup.c \
         sge_ja_task.c \
         sge_job.c \
         sge_mailrec.c \
         sge_manop.c \
         sge_object.c \
         sge_parse_date_time.c \
         sge_path_alias.c \
         sge_pe.c \
         sge_pe_task.c \
         sge_queue.c \
         sge_range.c \
         sge_resource.c \
         sge_report.c \
         sge_schedd_conf.c \
         sge_sharetree.c \
         sge_suser.c \
         sge_usage.c \
         sge_usermap.c \
         sge_userprj.c \
         sge_userset.c \
         sge_utility.c \
         sge_var.c 

SGEOBJLIB_OBJS = $(SGEOBJLIB_SRC:.c=.o)

###############################################################################
# global rules: all, clean, depend, messages
###############################################################################
sgeobjlib: $(SGEOBJLIB) $(ALLLISTSLIB)

sgeobjlib_clean:
		$(RM) $(SGEOBJLIB_OBJS) libsgeobj.a libsgeobj$(SHAREDEXT) libsgeobjd.a libsgeobjd$(SHAREDEXT) sge_all_listsL.o

sgeobjlib_depend:
		$(DEPEND) -f$(SGEOBJLIB_DIR)/sgeobj_dependencies $(SGEOBJLIB_DIR)/*.c

sgeobjlib_messages:
	@echo Creating $@
	@for i in `ls $(SGEOBJLIB_DIR)/*.c`; do \
	   $(CC) $(CFLAGS) -DEXTRACT_MESSAGES -E $$i | $(XGETTEXT) - ;\
	done

###############################################################################
# rules to build library
###############################################################################
libsgeobj.a:	$(SGEOBJLIB_OBJS) 
	$(AR) $(ARFLAGS) libsgeobj.a $(SGEOBJLIB_OBJS)
	$(RANLIB) libsgeobj.a

libsgeobj$(SHAREDEXT): $(SGEOBJLIB_OBJS) $(ALLLISTSLIB) $(COMMLIB) $(CULLLIB) $(UTILIB) $(RMONLIB)
	$(SHAREDLD) $(SHARED_LFLAGS) -o libsgeobj$(SHAREDEXT) $(SGEOBJLIB_OBJS) -lsgeobjd -lcom -lcull -luti -lrmon $(LIBS) -lc

libsgeobjd.a:	sge_all_listsL.o
	$(AR) $(ARFLAGS) libsgeobjd.a sge_all_listsL.o
	$(RANLIB) libsgeobjd.a

libsgeobjd$(SHAREDEXT): sge_all_listsL.o
	$(SHAREDLD) $(SHARED_LFLAGS) -o libsgeobjd$(SHAREDEXT) sge_all_listsL.o

###############################################################################
# include dependencies for object files generated by sge_depend
###############################################################################
include $(SGEOBJLIB_DIR)/sgeobj_dependencies

###############################################################################
# rules to build object codes
###############################################################################
