###############################################################################
# rules to build test applications 
###############################################################################

test_sge_object: test_sge_object.o $(GDILIB) $(SGEOBJLIB) sge_all_listsL.o $(CULLLIB) $(COMMLIB) $(UTILIB) $(RMONLIB)
	$(CC) $(CFLAGS) -o test_sge_object $(LFLAGS) test_sge_object.o sig_handlers.o $(GDILIB) $(SGEOBJLIB) sge_all_listsL.o $(CULLLIB) $(COMMLIB) $(UTILIB) $(RMONLIB) $(LIBS)

test_sge_object.o: $(SGEOBJLIB_DIR)/test_sge_object.c
	$(CC) $(CFLAGS) -c $(SGEOBJLIB_DIR)/test_sge_object.c


